  "scripts": {
    "ng": "ng",
    "syncSchema": "graphql-code-generator --config ./scripts/codegen.ts",
    "prepare": "npm run syncSchema",
    "start": "ng serve --configuration development",
    "start:prod": "ng serve --configuration production",
