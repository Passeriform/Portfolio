    "lint": "npm run codelint && npm run stylelint",
    "lint:fix": "npm run codelint:fix && npm run stylelint:fix",
    "cpcname": "cp CNAME dist/passeriform",
    "publish:gh-pages": "npm run build:prod && npm run cpcname && angular-cli-ghpages --repo git@github.com:Passeriform/passeriform.github.io --message 'Deploy: New publish to gh-pages.' --dir dist/passeriform --branch master"
